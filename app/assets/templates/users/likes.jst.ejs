<% likeCount = SoundStorm.currentUser.likes.length %>

<h2><%= likeCount %> <%= likeCount == 1 ? "Like" : "Likes" %></h2>

<ul>

	<% SoundStorm.currentUser.likes.each(function(like) { %>

		<li>"<a href="#/tracks/<%= like.get("track").id %>"><%= like.get("track").name %>"</a> <audio src="<%= like.get("track").audio_url %>" controls></audio></li>

	<% }); %>

</ul>