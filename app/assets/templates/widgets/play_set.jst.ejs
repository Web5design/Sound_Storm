<div class="profile-widget play-set" data-play-set-id="<%= playSet.id %>">
	<p><%= playSet.escape("name") %></p>
	<p>Includes: <% if (playSet.get("tracks").length == 0) { %> 
			<%= "Nothing!" %>
		<% } %>
		
		<% _(playSet.get("tracks")).each(function(track) { %>
		<%= track.name + " " %>
		<% }); %>
	</p>
	<button>Play</button>
	<a href="play_sets/<%= playSet.id %>/edit">Edit</a>
	<button class="remove">Remove</button>
</div>