<div class="track-show-info">
	<img src="<%= track.escape("image_url") %>">
	<div class="track-info-container">
		<h2><%= track.escape('name') %></h2>
		<p>Released By: <%= track.get("owner").username %></p>
		<p><span class="icon-tag"></span><%= track.escape("category") %></p>
	</div>
</div>

<%= JST['tracks/_player']({ track: track }) %>

